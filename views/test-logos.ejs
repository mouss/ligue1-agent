<!DOCTYPE html>
<html lang="fr">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Test des Logos</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet">
    <style>
        .logo-test {
            border: 1px solid #ddd;
            padding: 1rem;
            margin-bottom: 1rem;
            border-radius: 8px;
        }
        .team-logo {
            width: 64px;
            height: 64px;
            object-fit: contain;
            margin-right: 1rem;
        }
    </style>
</head>
<body>
    <div class="container mt-4">
        <h1 class="mb-4">Test des Logos</h1>
        <div class="row">
            <% logos.forEach(function(item) { %>
                <div class="col-md-4 mb-3">
                    <div class="logo-test">
                        <div class="d-flex align-items-center">
                            <img src="<%= item.logo %>" alt="<%= item.team %>" class="team-logo">
                            <div>
                                <h5 class="mb-0"><%= item.team %></h5>
                                <small class="text-muted"><%= item.logo %></small>
                            </div>
                        </div>
                    </div>
                </div>
            <% }); %>
        </div>
    </div>
</body>
</html>
